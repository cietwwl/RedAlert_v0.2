package com.youxigu.dynasty2.entity.domain;

/**
 * 兵种实体定义
 * 
 */
public class Army extends Entity {
	/**
	 * 
	 */
	private static final long serialVersionUID = 5353015806852876452L;

	private String armyName; // 军队名称

	private String armyDesc; // 军队介绍

	private String armyType; // 军队类型 步、骑、弓、车、器

	public String getArmyName() {
		return armyName;
	}

	public void setArmyName(String armyName) {
		this.armyName = armyName;
	}

	public String getArmyDesc() {
		return armyDesc;
	}

	public void setArmyDesc(String armyDesc) {
		this.armyDesc = armyDesc;
	}

	public String getArmyType() {
		return armyType;
	}

	public void setArmyType(String armyType) {
		this.armyType = armyType;
	}

	public String getEntityName() {
		return this.armyName;
	}

}
