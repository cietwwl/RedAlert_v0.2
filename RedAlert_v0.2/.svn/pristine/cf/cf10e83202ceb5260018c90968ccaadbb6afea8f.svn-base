package com.youxigu.dynasty2.entity.domain;

/**
 * 随机属性
 * 
 * @author fengfeng
 *
 */
public class RandomItemProperty {
	private String propName;// 属性名称
	private int minValue;// 属性最小值
	private int maxValue;// 属性最大值
	private boolean abs;// true ,value是绝对值,false,value是百分比
	private int weightVal;// 概率权重
	private int specialWeightVal;// 特殊属性出现的概率 千分比

	public RandomItemProperty() {
	}

	public String getPropName() {
		return propName;
	}

	public void setPropName(String propName) {
		this.propName = propName;
	}

	public boolean isAbs() {
		return abs;
	}

	public void setAbs(boolean abs) {
		this.abs = abs;
	}

	public int getMinValue() {
		return minValue;
	}

	public void setMinValue(int minValue) {
		this.minValue = minValue;
	}

	public int getMaxValue() {
		return maxValue;
	}

	public void setMaxValue(int maxValue) {
		this.maxValue = maxValue;
	}

	public int getWeightVal() {
		return weightVal;
	}

	public void setWeightVal(int weightVal) {
		this.weightVal = weightVal;
	}

	public int getSpecialWeightVal() {
		return specialWeightVal;
	}

	public void setSpecialWeightVal(int specialWeightVal) {
		this.specialWeightVal = specialWeightVal;
	}

}
