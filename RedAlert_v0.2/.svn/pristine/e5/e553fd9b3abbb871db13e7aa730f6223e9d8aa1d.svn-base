package com.youxigu.dynasty2.common.domain;

import java.sql.Timestamp;

public class ServerInfo implements java.io.Serializable {
    private static final long serialVersionUID = -51488996827896734L;
	private String serverId;
	/**
	 * 开服日期
	 */
	private Timestamp dttm;

	private String serverName;
	/**
	 * 当前正在使用的跨服战server ip port, ip:port格式
	 */
	private String interServer;
	/**
	 * 一个跨服战结束后将要使用的跨服战server ip port, ip:port格式
	 */
	private String interServer2;

	public ServerInfo() {

	}

	public ServerInfo(String serverId, Timestamp dttm) {
		this.serverId = serverId;
		this.dttm = dttm;
	}

	public String getServerId() {
		return serverId;
	}

	public void setServerId(String serverId) {
		this.serverId = serverId;
	}

	public Timestamp getDttm() {
		return dttm;
	}

	public void setDttm(Timestamp dttm) {
		this.dttm = dttm;
	}

	public String getServerName() {
		return serverName;
	}

	public void setServerName(String serverName) {
		this.serverName = serverName;
	}

	public String getInterServer() {
		return interServer;
	}

	public void setInterServer(String interServer) {
		this.interServer = interServer;
	}

	public String getInterServer2() {
		return interServer2;
	}

	public void setInterServer2(String interServer2) {
		this.interServer2 = interServer2;
	}

}
