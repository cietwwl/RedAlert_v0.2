package com.youxigu.dynasty2.combat.domain;

/**
 * 战斗公式系数表
 * @author Dagangzi
 *
 */
public class CombatFactor implements java.io.Serializable {
	public static final String F_HARM = "HARM";//伤害公式参数  
	public static final String F_HIT = "HIT";//命中率公式参数
	public static final String F_CRITICAL = "CRITICAL";//暴击率公式参数	
	public static final String F_SHIELD = "SHIELD";//护盾
	public static final String F_CRITICAL_HARM = "CRITICAL_HARM";//暴击伤害公式参数
	public static final String F_COMBAT = "F_COMBAT";//战斗力计算系数
	public static final String F_MORALE_MAX = "F_MORALE_MAX";//士气上限
	private String formula;
	private double para1;
	private double para2;
	private double para3;
	private double para4;

	public String getFormula() {
		return formula;
	}

	public void setFormula(String formula) {
		this.formula = formula;
	}

	public double getPara1() {
		return para1;
	}

	public void setPara1(double para1) {
		this.para1 = para1;
	}

	public double getPara2() {
		return para2;
	}

	public void setPara2(double para2) {
		this.para2 = para2;
	}

	public double getPara3() {
		return para3;
	}

	public void setPara3(double para3) {
		this.para3 = para3;
	}

	public double getPara4() {
		return para4;
	}

	public void setPara4(double para4) {
		this.para4 = para4;
	}

}
