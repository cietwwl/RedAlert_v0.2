package com.youxigu.dynasty2.entity.domain;

/**
 * 武将技能效果约束
 * 
 * @author Dagangzi
 *
 */
public class HeroSkillEffectLimit implements java.io.Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = -3475032693262559801L;
	private int id;
	private int effId;
	private String limitType;
	private int para1;
	private String para2;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getEffId() {
		return effId;
	}

	public void setEffId(int effId) {
		this.effId = effId;
	}

	public String getLimitType() {
		return limitType;
	}

	public void setLimitType(String limitType) {
		this.limitType = limitType;
	}

	public int getPara1() {
		return para1;
	}

	public void setPara1(int para1) {
		this.para1 = para1;
	}

	public String getPara2() {
		return para2;
	}

	public void setPara2(String para2) {
		this.para2 = para2;
	}
}
