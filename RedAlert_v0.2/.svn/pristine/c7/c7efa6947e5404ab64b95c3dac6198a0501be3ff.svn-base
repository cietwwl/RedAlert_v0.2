package com.youxigu.dynasty2.mission.domain;

import java.io.Serializable;

/**
 * 任务类型
 * 
 * @author Administrator
 *
 */
public class MissionType implements Serializable {
	private static final long serialVersionUID = -5242168688365050645L;
	public static final int LIMIT_TYPE_USER_LV = 1;
	public static final int LIMIT_TYPE_CASTLE_LV = 2;
	private String missionChildType;// 任务子类型，//pk

	private String missionChildTypeName;// 任务子类型名称

	private String missionType;// 任务类型

	private int limitType;// =1 用户级别 =2城池级别

	private int limitValue;

	public String getMissionType() {
		return missionType;
	}

	public void setMissionType(String missionType) {
		this.missionType = missionType;
	}

	public String getMissionChildType() {
		return missionChildType;
	}

	public void setMissionChildType(String missionChildType) {
		this.missionChildType = missionChildType;
	}

	public String getMissionChildTypeName() {
		return missionChildTypeName;
	}

	public void setMissionChildTypeName(String missionChildTypeName) {
		this.missionChildTypeName = missionChildTypeName;
	}

	public int getLimitType() {
		return limitType;
	}

	public void setLimitType(int limitType) {
		this.limitType = limitType;
	}

	public int getLimitValue() {
		return limitValue;
	}

	public void setLimitValue(int limitValue) {
		this.limitValue = limitValue;
	}

}
