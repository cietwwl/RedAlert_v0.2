package com.youxigu.dynasty2.hero.proto;

import java.io.Serializable;

public class HeroCardAndDebris implements Serializable {
	private static final long serialVersionUID = -6614652600564103274L;
	private long heroId;
	private int sysHeroId;
	private int cardNum;
	private int debrisNum;

	public HeroCardAndDebris(long heroId, int sysHeroId, int cardNum,
			int debrisNum) {
		this.heroId = heroId;
		this.sysHeroId = sysHeroId;
		this.cardNum = cardNum;
		this.debrisNum = debrisNum;
	}

	public long getHeroId() {
		return heroId;
	}

	public void setHeroId(long heroId) {
		this.heroId = heroId;
	}

	public int getSysHeroId() {
		return sysHeroId;
	}

	public void setSysHeroId(int sysHeroId) {
		this.sysHeroId = sysHeroId;
	}

	public int getCardNum() {
		return cardNum;
	}

	public void setCardNum(int cardNum) {
		this.cardNum = cardNum;
	}

	public int getDebrisNum() {
		return debrisNum;
	}

	public void setDebrisNum(int debrisNum) {
		this.debrisNum = debrisNum;
	}

}
