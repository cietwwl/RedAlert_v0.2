<beans xmlns="http://www.springframework.org/schema/beans"
	     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xmlns:aop="http://www.springframework.org/schema/aop"
	     xmlns:tx="http://www.springframework.org/schema/tx"
		 xmlns:flex="http://www.springframework.org/schema/flex"
	     xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
		   http://www.springframework.org/schema/flex http://www.springframework.org/schema/flex/spring-flex-1.0.xsd">

	
	<bean id="npcDao" class="com.youxigu.dynasty2.npc.dao.impl.NPCDao" >
		<property name="sqlMapClientTemplate"><ref bean="sqlMapClientTemplate" /></property>
	</bean>

	<bean id="npcService" class="com.youxigu.dynasty2.npc.service.impl.NPCServiceImpl" >
		<property name="npcDao"><ref bean="npcDao" /></property>
		<property name="entityService"><ref bean="entityService" /></property>
	</bean>

</beans>

