package com.youxigu.dynasty2.hero.domain;

import java.io.Serializable;

/**
 * 武将强化限制
 * 
 * @author Dagangzi
 *
 */
public class StrongLimit implements Serializable {
	private static final long serialVersionUID = -5333098067453701144L;
	private int strongNum;// 成长值/10
	private int itemEntId;// 需要道具entId
	private int itemNum;// 需要道具数量
	private int moneyNum;// 需要铜币

	public int getStrongNum() {
		return strongNum;
	}

	public void setStrongNum(int strongNum) {
		this.strongNum = strongNum;
	}

	public int getItemEntId() {
		return itemEntId;
	}

	public void setItemEntId(int itemEntId) {
		this.itemEntId = itemEntId;
	}

	public int getItemNum() {
		return itemNum;
	}

	public void setItemNum(int itemNum) {
		this.itemNum = itemNum;
	}

	public int getMoneyNum() {
		return moneyNum;
	}

	public void setMoneyNum(int moneyNum) {
		this.moneyNum = moneyNum;
	}
}
