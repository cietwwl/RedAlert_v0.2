package com.youxigu.dynasty2.entity.domain;import java.io.Serializable;/** *  * 装备强化规则：按装备等级确定强化规则<br> * 装备强化规则修改为按照装备的品质、等级来确认消耗 去掉了一些无用字段<br> * 排序规则 equipColor*10000+level *  * @author fengfeng *  *  */public class EquipLevelUpRule implements Serializable,		Comparable<EquipLevelUpRule> {	private static final long serialVersionUID = -5955777391554074794L;	// 复合主键(equipColor, level)	private int equipColor;// 装备品质	private int level;// 装备等级	private int lvUpGoldNum;// 消耗的金矿数量	private int entId;// 需要的道具ID	private int entNum;// 需要的道具数量	private int entId2;// 需要的道具ID2	private int entNum2;// 需要的道具数量2	private int destroyEntId;// 分解获得的东西	private int destroyEntNum;	private int destroyEntId2;	private int destroyEntNum2;	private int destroyGoldNum;	public int getEquipColor() {		return equipColor;	}	public void setEquipColor(int equipColor) {		this.equipColor = equipColor;	}	public int getLevel() {		return level;	}	public void setLevel(int level) {		this.level = level;	}	public int getEntId() {		return entId;	}	public void setEntId(int entId) {		this.entId = entId;	}	public int getEntNum() {		return entNum;	}	public void setEntNum(int entNum) {		this.entNum = entNum;	}	public int getEntId2() {		return entId2;	}	public void setEntId2(int entId2) {		this.entId2 = entId2;	}	public int getEntNum2() {		return entNum2;	}	public void setEntNum2(int entNum2) {		this.entNum2 = entNum2;	}	public int getLvUpGoldNum() {		return lvUpGoldNum;	}	public void setLvUpGoldNum(int lvUpGoldNum) {		this.lvUpGoldNum = lvUpGoldNum;	}	public int getDestroyEntId() {		return destroyEntId;	}	public void setDestroyEntId(int destroyEntId) {		this.destroyEntId = destroyEntId;	}	public int getDestroyEntNum() {		return destroyEntNum;	}	public void setDestroyEntNum(int destroyEntNum) {		this.destroyEntNum = destroyEntNum;	}	public int getDestroyEntId2() {		return destroyEntId2;	}	public void setDestroyEntId2(int destroyEntId2) {		this.destroyEntId2 = destroyEntId2;	}	public int getDestroyEntNum2() {		return destroyEntNum2;	}	public void setDestroyEntNum2(int destroyEntNum2) {		this.destroyEntNum2 = destroyEntNum2;	}	public int getDestroyGoldNum() {		return destroyGoldNum;	}	public void setDestroyGoldNum(int destroyGoldNum) {		this.destroyGoldNum = destroyGoldNum;	}	public int getId() {		return EquipLevelUpRule.getEquipId(this.equipColor, this.getLevel());	}	public static int getEquipId(int equipColor, int level) {		return equipColor * 10000 + level;	}	@Override	public int compareTo(EquipLevelUpRule o) {		return getId() - o.getId();	}}